{
  "name": "swiss-army-utils",
  "version": "0.7.1",
  "description": "General typescript utils.",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "default": "./dist/esm/*.js",
      "import": "./dist/esm/*.js",
      "module": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "private": false,
  "scripts": {
    "test": "jest",
    "test:types": "tsc --noEmit",
    "lint": "eslint ./src",
    "build": "rimraf dist && tsc -b tsconfig.esm.build.json -b tsconfig.cjs.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm test && npm run lint && npm run build",
    "preversion": "npm run prepare",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "typescript",
    "utilities"
  ],
  "author": "Mattias Ahls√©n",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "zod": "^4.0.5",
    "type-fest": "^4.41.0"
  }
}
